https://www.hackerrank.com/challenges/compare-the-triplets/problem?isFullScreen=true

#include<iostream>
using namespace std;
int main()
{
    int al=0,bob=0;
    //cin>>n;
    int a[5],b[5];
   for(int i=0;i<3;i++)
   {
        cin>>a[i];
   }
   for(int i=0;i<3;i++)
   {
        cin>>b[i];
        
    }
    for(int i=0;i<3;i++)
   {
       if(a[i]<b[i])
       {
       bob=bob+1;
       }
       else if(a[i]>b[i])
       {
           al=al+1; 
       }    
   }
      cout<<al<<" "<<bob;
   return 0;
    
    
}
